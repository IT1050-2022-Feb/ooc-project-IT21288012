//Co - Admin
#include<string.h>

class co_admin
{
	private :
		int co_admin_ID;
		char co_admin_email[50];
		char co_admin_username[50];
		char co_admin_password[50];
		
	public :
		co_admin();
		co_admin(int co_admin_ID, char co_admin_email[], char co_admin_username[],char co_admin_password[]);	
		void login();
		check_payment();
		void manage_users();
		void manage_job();
		 
};

